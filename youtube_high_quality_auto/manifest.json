
{
  "manifest_version": 3,
  "name": "YouTube High Quality Auto",
  "version": "1.0",
  "description": "Automatically sets YouTube videos to the highest quality.",
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png"
    }
  }
}
